{
  "name": "code-stack",
  "private": true,
  "scripts": {
    "build": "turbo run build ",
    "dev": "turbo run dev ",
    "package:dev": "turbo run build:dev --filter='./packages/*'",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "check-types": "turbo run check-types"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "prettier": "^3.6.1",
    "tsup": "^8.5.0",
    "turbo": "^2.5.4",
    "typescript": "5.8.3"
  },
  "packageManager": "pnpm@10.12.3",
  "engines": {
    "node": ">=18"
  },
  "module": "index.ts",
  "type": "module"
}
